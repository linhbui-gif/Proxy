extends includes/layouts/Layout.pug

block pages
    .MyIp.pt-5.pb-5
        .container
            h1.title-component-h1.mb-0
                | MY IP:
                span.orange.font-weight-bold.ml-2 58.187.249.103
            p.description-common
                 |With this service you can detect your IP-address and get detailed information about it.
                 br
                 |In addition to the IP information, the data that your browser sends to the server is displayed here.
            .row.mt-5
                .col-12.col-lg-6
                    dl.inline-dotted
                        dt IP
                        dd 58.187.249.103
                    dl.inline-dotted
                        dt My Country
                        dd VietNam(VN)
                    dl.inline-dotted
                        dt Zipcode
                        dd
                    each item in [1,2,3,4,5]
                        dl.inline-dotted
                            dt My Country
                            dd VietNam(VN)
                .col-12.col-lg-6
                    a(href="javascript:void(0)" data-toggle="modal" data-target=".bd-example-modal-lg")
                        img(src="https://static-maps.yandex.ru/1.x/?ll=105.8516,21.0313&size=450,170&z=9&l=map&pt=105.8516,21.0313" width="100%")
            .myip_server_head.color-light HTTP request sent by your browser:
            each item in [1,2,3,4,5]
                dl.inline-dotted
                    dt My Country
                    dd VietNam(VN)
            #exampleModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true').bd-example-modal-lg
                .modal-dialog(role='document').modal-dialog-centered.modal-lg
                    .modal-content
                        .modal-body
                            iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3723.9822429853607!2d105.7955849154025!3d21.033396492994875!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3135ab47b42f0543%3A0x85522387d91f54d4!2zxJAuIEPhuqd1IEdp4bqleSwgQ-G6p3UgR2nhuqV5LCBIw6AgTuG7mWksIFZp4buHdCBOYW0!5e0!3m2!1svi!2s!4v1663427036330!5m2!1svi!2s' width='100%' height='450' style='border:0;' allowfullscreen='' loading='lazy' referrerpolicy='no-referrer-when-downgrade')
