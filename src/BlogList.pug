extends includes/layouts/Layout.pug

block pages
    .blog-list.pt-5.pb-5
        .container
            h1.title-component-h1 BLOG ABOUT PROXY
            +ListGroupComponent({ className:'blog-tags', type:'horizon ' ,array: dataNavigation})
            .row
                each item in [1,2,3,4,5,6,7]
                    .col-12
                       +CardComponent({
                           className:'hozizon mt-5',
                           hozizon:true,
                           circle:'circle',
                           title:'Рекомендации по аккаунтам Instagram',
                           desc:'ля защиты своего аккаунта Instagram от бана, также блокировки на действия, вам необходимо выполнять предоставленные рекомендации. Выполняя их, можно продлить работу вашего аккаунта.',
                           tag:'instagram',
                           src: 'https://proxy6.net/uploads/img/20180320/instagram-accounts-39263.jpg'
                       })
                +Pagination( {defaultActive: 1} )